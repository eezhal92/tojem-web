{
  "name": "tojem-web",
  "version": "0.0.1",
  "private": true,
  "main": "server.js",
  "scripts": {
    "test": "npm run lint && jest",
    "db:migrate": "sequelize db:migrate --url $(./scripts/db-url)",
    "db:migrate:undo": "sequelize db:migrate:undo --url $(./scripts/db-url)",
    "dev": "cross-env NODE_PATH=. nodemon --exec 'node server.js'",
    "lint": "eslint ."
  },
  "engines": {
    "node": ">= 8",
    "npm": ">= 5"
  },
  "dependencies": {
    "@std/esm": "^0.17.3",
    "body-parser": "^1.18.2",
    "cross-env": "^5.1.1",
    "dotenv": "^4.0.0",
    "ejs": "^2.5.7",
    "express": "^4.16.2",
    "mysql2": "^1.4.2",
    "sequelize": "^4.19.0",
    "sequelize-cli": "^3.2.0"
  },
  "devDependencies": {
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.12.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.3.2",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "jest": "^21.2.1",
    "nodemon": "^1.12.1"
  },
  "@std/esm": {
    "cjs": true,
    "esm": "js"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "env": {
      "node": true,
      "browser": false,
      "jest/globals": true
    },
    "plugins": [
      "jest"
    ]
  },
  "babel": {
    "presets": [
      "env"
    ]
  }
}
